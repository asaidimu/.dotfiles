! Select the font to be used.
URxvt.font: xft:SauceCodePro NF, xft:Font Awesome 5 Free

!use visual bell on receipt of a bell character
URxvt.visualBell: true

!disable the scrollbar
URxvt.scrollBar: false

!blink the cursor
URxvt.cursorBlink: true

!Make the cursor underlined
URxvt.cursorUnderline: true

!Specifies the program to be URxvtarted with a URL argument
URxvt.url-launcher: chromium

!Internal border width
URxvt.internalBorder: 1

!External border width
URxvt.externalBorder: 1

! Window Size
URxvt.geometry: 95x24

! ??
! URxvt.depth: 32

!Show Scroll Bar
URxvt*scrollBar:false

! ...
URxvt*mouseWheelScrollPage: false

URxvt*saveLines: 5000

!! Turn on/off pseudo-transparency by using the root pixmap as background.
URxvt.transparent: true

!! Darken (0 .. 99) or lighten (101 .. 200) the transparent background.
URxvt.shading: 60

st.alpha: 0.9

